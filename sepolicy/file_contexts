# camera
/data/cam_socket1                                           u:object_r:camera_socket:s0

# cmhw
/sys/devices/virtual/graphics/fb0/kcal                      u:object_r:display_sysfs:s0
/sys/devices/virtual/timed_output/vibrator/vtg_level        u:object_r:vibeamp_sysfs:s0

# mpdecision
/data/system/default_values                                 u:object_r:mpctl_data_file:s0
/dev/socket/mpctl                                           u:object_r:mpctl_socket:s0

# sensors
/dev/light_sensor                                           u:object_r:sensors_device:s0

# thermal
/system/bin/thermal-engine                                  u:object_r:thermal-engine_exec:s0

# wcnss - zte wifi mac file
/persist/wifimac.dat                                        u:object_r:wcnss_persist_file:s0
